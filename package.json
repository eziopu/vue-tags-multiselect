{
  "name": "vue-tags-multiselect",
  "version": "1.1.0-docs1",
  "description": "This is a complex multi-selector.",
  "author": "eziopu <atkdet@gmail.com>",
  "keywords": [
    "multiselect",
    "dropdown",
    "select",
    "tag",
    "vue",
    "vue3"
  ],
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/vue-tags-multiselect.umd.cjs",
  "module": "./dist/vue-tags-multiselect.js",
  "exports": {
    ".": {
      "import": "./dist/vue-tags-multiselect.js",
      "require": "./dist/vue-tags-multiselect.umd.cjs"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite --config demo/vite.config.js",
    "build": "vite build",
    "build:doc": "vite build --config demo/vite.config.js",
    "build:check": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "deploy": "gh-pages -d docs"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "element-plus": "^2.8.3",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "gh-pages": "^6.1.1",
    "intro.js": "^7.2.0",
    "prettier": "^3.0.3",
    "sass": "^1.69.5",
    "vite": "^4.4.11",
    "vue-i18n": "^9.8.0",
    "vue-router": "^4.4.2"
  },
  "bugs": {
    "url": "https://github.com/eziopu/vue-tags-multiselect/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eziopu/vue-tags-multiselect.git"
  }
}
